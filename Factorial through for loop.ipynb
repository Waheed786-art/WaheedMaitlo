   "source": [
    "# Factorial through for loop\n",
    "# 1st Method\n",
    "n = 5\n",
    "f = 1\n",
    "for i in range(n, 0,-1):\n",
    "    f *= i\n",
    "print(\"The factorial of 5 is\", f)\n",
    "\n",
    "#2nd Method\n",
    "num = 4\n",
    "fac = 1\n",
    "for i in range(1, num+1):\n",
    "    fac = fac*i\n",
    "\n",
    "print(\"The factorial of 4 is\",fac)"
   ]
  }
